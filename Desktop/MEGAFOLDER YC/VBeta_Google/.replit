run = "npm run dev"
entrypoint = "index.html"

[nix]
channel = "stable-22_11"

[deployment]
run = ["sh", "-c", "npm run build && npm run preview"]
deploymentTarget = "static"
publicDir = "dist"

[languages.typescript]
pattern = "**/*.{ts,tsx}"

[languages.typescript.languageServer]
start = "typescript-language-server --stdio"

